 {
  "manifest_version": 2,
  "name": "chas-correct",
  "version": "0.1",
    "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": [ "dictionary.js","content.js"  ],
      "all_frames":true,
      "run_at": "document_idle"
    }
  ],
  "background": {
  },
  "permissions": [
   ],

      "browser_action": {
        "default_title": "chas-correct"
    }
    
}
